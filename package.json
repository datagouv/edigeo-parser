{
  "name": "@etalab/edigeo-parser",
  "version": "0.9.1",
  "description": "Blazing fast parser for EDIGEO files",
  "main": "./lib",
  "repository": "git@github.com:etalab/edigeo-parser.git",
  "author": "Jérôme Desboeufs <jerome.desboeufs@data.gouv.fr>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "bin": {
    "edigeo-parse": "bin/cli.js"
  },
  "scripts": {
    "lint": "xo",
    "test": "nyc ava",
    "test:unit": "ava",
    "codecov": "codecov -f coverage/coverage-final.json"
  },
  "dependencies": {
    "@turf/intersect": "^6.1.3",
    "@turf/turf": "^5.1.6",
    "debug": "^4.1.1",
    "decompress": "^4.2.0",
    "epsg-index": "^0.28.0",
    "get-stdin": "^7.0.0",
    "is-buffer": "^2.0.3",
    "lodash": "^4.17.15",
    "proj4": "^2.5.0",
    "split2": "^3.1.1"
  },
  "devDependencies": {
    "ava": "^2.2.0",
    "codecov": "^3.5.0",
    "nyc": "^14.1.1",
    "xo": "^0.24.0"
  },
  "xo": {
    "space": 2,
    "semicolon": false,
    "rules": {
      "curly": "off",
      "prefer-destructuring": "off"
    }
  },
  "engines": {
    "node": ">= 8"
  }
}
